{
  "version": 2,
  "builds": [
    {
      "src": "myblog/manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "todo/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myblog/manage.py"
    }
  ],
  "plugins": ["django-static"]
}
